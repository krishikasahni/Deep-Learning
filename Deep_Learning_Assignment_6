      "source": [
        "def decode_sequence(input_seq):\n",
        "    states_value = encoder_model.predict(input_seq, verbose=0)\n",
        "\n",
        "    target_seq = np.array([[spa_tokenizer.word_index['<start>']]])\n",
        "    decoded_sentence = \"\"\n",
        "    used_words = set()\n",
        "    for _ in range(20):\n",
        "        output_tokens, h, c = decoder_model.predict([target_seq] + states_value, verbose=0)\n",
        "        top_k = 3\n",
        "        probs = output_tokens[0, -1, :]\n",
        "        top_indices = probs.argsort()[-top_k:][::-1]\n",
        "\n",
        "        sampled_token_index = None\n",
        "        for idx in top_indices:\n",
        "            word = spa_tokenizer.index_word.get(idx, '')\n",
        "            if word not in used_words:\n",
        "                sampled_token_index = idx\n",
        "                used_words.add(word)\n",
        "                break\n",
        "\n",
        "        if sampled_token_index is None:\n",
        "            sampled_token_index = top_indices[0]\n",
        "\n",
        "        sampled_word = spa_tokenizer.index_word.get(sampled_token_index, '')\n",
        "\n",
        "        if sampled_word == '<end>' or sampled_word == '':\n",
        "            break\n",
        "\n",
        "        decoded_sentence += \" \" + sampled_word\n",
        "\n",
        "        target_seq = np.array([[sampled_token_index]])\n",
        "        states_value = [h, c]\n",
        "\n",
        "    return decoded_sentence.strip()"
      ],
